<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2020-01-14T08:02:01Z</date>
    <groups>
        <group>
            <name>Templates/Network devices</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template Netgear ReadyNAS</template>
            <name>Template Netgear ReadyNAS</name>
            <description>NetGear ReadyNAS 2100</description>
            <groups>
                <group>
                    <name>Templates/Network devices</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Disk</name>
                </application>
                <application>
                    <name>Fan</name>
                </application>
                <application>
                    <name>General</name>
                </application>
                <application>
                    <name>Temperature</name>
                </application>
                <application>
                    <name>Volume</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>S/N</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.4526.22.2.0</snmp_oid>
                    <key>nasMgrSerialNUM.0</key>
                    <delay>30</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <port>{$SNMP_PORT}</port>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Firmware</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.4526.18.1.0</snmp_oid>
                    <key>nasMgrSoftwareVersion.0</key>
                    <delay>30</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <port>{$SNMP_PORT}</port>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>Discovery Disk</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.4526.18.3.1.1]</snmp_oid>
                    <key>Disk</key>
                    <delay>1h</delay>
                    <port>{$SNMP_PORT}</port>
                    <item_prototypes>
                        <item_prototype>
                            <name>Disk #{#SNMPVALUE} chanel</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.4526.18.3.1.2.{#SNMPINDEX}</snmp_oid>
                            <key>DiskChanel[{#SNMPVALUE}]</key>
                            <delay>30</delay>
                            <port>{$SNMP_PORT}</port>
                            <applications>
                                <application>
                                    <name>Disk</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Disk #{#SNMPVALUE} model</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.4526.18.3.1.3.{#SNMPINDEX}</snmp_oid>
                            <key>DiskModel[{#SNMPVALUE}]</key>
                            <delay>30</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <port>{$SNMP_PORT}</port>
                            <applications>
                                <application>
                                    <name>Disk</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Disk #{#SNMPVALUE} state</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.4526.18.3.1.4.{#SNMPINDEX}</snmp_oid>
                            <key>DiskState[{#SNMPVALUE}]</key>
                            <delay>30</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <port>{$SNMP_PORT}</port>
                            <applications>
                                <application>
                                    <name>Disk</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{regexp(&quot;(Offline)&quot;)}=1</expression>
                                    <name>Disk #{#SNMPVALUE} on {HOST.NAME} is Offline</name>
                                    <priority>DISASTER</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Disk #{#SNMPINDEX} temperature</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.4526.18.3.1.5.{#SNMPINDEX}</snmp_oid>
                            <key>DiskTemperature[{#SNMPVALUE}]</key>
                            <delay>30</delay>
                            <value_type>FLOAT</value_type>
                            <units>℃</units>
                            <port>{$SNMP_PORT}</port>
                            <applications>
                                <application>
                                    <name>Disk</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JAVASCRIPT</type>
                                    <params>return (Math.floor(((value - 32) * 5 / 9) * 10) / 10)</params>
                                </step>
                            </preprocessing>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last(0)}&gt;65</expression>
                                    <name>Disk #{#SNMPVALUE} temperature is High on {HOST.NAME}</name>
                                    <priority>AVERAGE</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Disk #{#SNMPINDEX} Temperature</name>
                            <show_work_period>NO</show_work_period>
                            <show_triggers>NO</show_triggers>
                            <graph_items>
                                <graph_item>
                                    <color>00C800</color>
                                    <item>
                                        <host>Template Netgear ReadyNAS</host>
                                        <key>DiskTemperature[{#SNMPVALUE}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Discovery Fan</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.4526.18.4.1.1]</snmp_oid>
                    <key>Fan</key>
                    <delay>1h</delay>
                    <port>{$SNMP_PORT}</port>
                    <item_prototypes>
                        <item_prototype>
                            <name>Fan #{#SNMPINDEX} RPM</name>
                            <type>SNMPV2</type>
                            <snmp_community>public</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.4526.18.4.1.2.{#SNMPINDEX}</snmp_oid>
                            <key>fanSpeed[{#SNMPINDEX}]</key>
                            <trends>180d</trends>
                            <units>RPM</units>
                            <applications>
                                <application>
                                    <name>Fan</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Discovery Temperature sensors</name>
                    <type>SNMPV2</type>
                    <snmp_community>public</snmp_community>
                    <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.4526.18.5.1.1]</snmp_oid>
                    <key>Temperature</key>
                    <delay>1h</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>Temperature #{#SNMPINDEX} sensor</name>
                            <type>SNMPV2</type>
                            <snmp_community>public</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.4526.18.5.1.2.{#SNMPINDEX}</snmp_oid>
                            <key>temperature[{#SNMPVALUE}]</key>
                            <delay>30</delay>
                            <value_type>FLOAT</value_type>
                            <units>℃</units>
                            <applications>
                                <application>
                                    <name>Temperature</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JAVASCRIPT</type>
                                    <params>return (Math.floor(((value - 32) * 5 / 9) * 10) / 10)</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Discovery Volume</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.4526.18.7.1.1]</snmp_oid>
                    <key>Volume</key>
                    <delay>1h</delay>
                    <port>{$SNMP_PORT}</port>
                    <item_prototypes>
                        <item_prototype>
                            <name>Volume #{#SNMPVALUE} FreeSpace</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.4526.18.7.1.6.{#SNMPINDEX}</snmp_oid>
                            <key>volumeFreeSpace[{#SNMPVALUE}]</key>
                            <delay>30</delay>
                            <value_type>FLOAT</value_type>
                            <port>{$SNMP_PORT}</port>
                            <applications>
                                <application>
                                    <name>Volume</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Volume #{#SNMPVALUE} Name</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.4526.18.7.1.2.{#SNMPINDEX}</snmp_oid>
                            <key>volumeName[{#SNMPVALUE}]</key>
                            <delay>30</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <port>{$SNMP_PORT}</port>
                            <applications>
                                <application>
                                    <name>Volume</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Volume #{#SNMPVALUE} RAID Level</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.4526.18.7.1.3.{#SNMPINDEX}</snmp_oid>
                            <key>volumeRAIDLevel[{#SNMPVALUE}]</key>
                            <delay>30</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <port>{$SNMP_PORT}</port>
                            <applications>
                                <application>
                                    <name>Volume</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Volume #{#SNMPVALUE} Size</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.4526.18.7.1.5.{#SNMPINDEX}</snmp_oid>
                            <key>volumeSize[{#SNMPVALUE}]</key>
                            <delay>30</delay>
                            <value_type>FLOAT</value_type>
                            <port>{$SNMP_PORT}</port>
                            <applications>
                                <application>
                                    <name>Volume</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Volume #{#SNMPVALUE} Status</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.4526.18.7.1.4.{#SNMPINDEX}</snmp_oid>
                            <key>volumeStatus[{#SNMPVALUE}]</key>
                            <delay>30</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <port>{$SNMP_PORT}</port>
                            <applications>
                                <application>
                                    <name>Volume</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{regexp(&quot;(UNPROTECTED|DEGRADED|DEAD|INACTIVE|UNKNOW)&quot;)}=1</expression>
                                    <name>Raid [{#SNMPVALUE}] on {HOST.NAME} - Problem</name>
                                    <priority>DISASTER</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>({Template Netgear ReadyNAS:volumeFreeSpace[{#SNMPVALUE}].last()} / {Template Netgear ReadyNAS:volumeSize[{#SNMPVALUE}].last()} * 100) &lt; 10</expression>
                            <name>Volume #{#SNMPVALUE} - FreeSpace is less than 10% on {HOST.NAME}</name>
                            <priority>HIGH</priority>
                        </trigger_prototype>
                    </trigger_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Volumen Size #{#SNMPVALUE}</name>
                            <width>200</width>
                            <yaxismax>0.0000</yaxismax>
                            <show_work_period>NO</show_work_period>
                            <show_triggers>NO</show_triggers>
                            <type>PIE</type>
                            <show_3d>YES</show_3d>
                            <graph_items>
                                <graph_item>
                                    <drawtype>BOLD_LINE</drawtype>
                                    <color>00C800</color>
                                    <item>
                                        <host>Template Netgear ReadyNAS</host>
                                        <key>volumeSize[{#SNMPVALUE}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <drawtype>BOLD_LINE</drawtype>
                                    <color>C80000</color>
                                    <item>
                                        <host>Template Netgear ReadyNAS</host>
                                        <key>volumeFreeSpace[{#SNMPVALUE}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
            </discovery_rules>
            <screens>
                <screen>
                    <name>ReadyNAS</name>
                    <hsize>2</hsize>
                    <vsize>2</vsize>
                    <screen_items>
                        <screen_item>
                            <resourcetype>20</resourcetype>
                            <style>0</style>
                            <resource>
                                <name>Volumen Size #{#SNMPVALUE}</name>
                                <host>Template Netgear ReadyNAS</host>
                            </resource>
                            <width>500</width>
                            <height>100</height>
                            <x>0</x>
                            <y>1</y>
                            <colspan>1</colspan>
                            <rowspan>1</rowspan>
                            <elements>0</elements>
                            <valign>0</valign>
                            <halign>0</halign>
                            <dynamic>0</dynamic>
                            <sort_triggers>0</sort_triggers>
                            <url/>
                            <application/>
                            <max_columns>3</max_columns>
                        </screen_item>
                        <screen_item>
                            <resourcetype>20</resourcetype>
                            <style>0</style>
                            <resource>
                                <name>Disk #{#SNMPINDEX} Temperature</name>
                                <host>Template Netgear ReadyNAS</host>
                            </resource>
                            <width>500</width>
                            <height>100</height>
                            <x>1</x>
                            <y>1</y>
                            <colspan>1</colspan>
                            <rowspan>1</rowspan>
                            <elements>0</elements>
                            <valign>0</valign>
                            <halign>0</halign>
                            <dynamic>0</dynamic>
                            <sort_triggers>0</sort_triggers>
                            <url/>
                            <application/>
                            <max_columns>3</max_columns>
                        </screen_item>
                    </screen_items>
                </screen>
            </screens>
        </template>
    </templates>
</zabbix_export>
