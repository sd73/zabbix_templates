--
-- EVPN IP-VRF table.
--
-- Read-write table for configuring IP-VRFs for EVPN.

  eltexEvpnIpVrfTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF EltexEvpnIpVrfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Read-write table for configuring IP-VRFs for EVPN."
    ::= { eltexEvpnIpVrf 1 }

  eltexEvpnIpVrfEntry OBJECT-TYPE
    SYNTAX      EltexEvpnIpVrfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Configuration for an IP-VRF."
    INDEX       { eltexEvpnIpVrfMrid }
      ::= { eltexEvpnIpVrfTable 1 }

  EltexEvpnIpVrfEntry ::= SEQUENCE {
      eltexEvpnIpVrfMrid                INTEGER,
      eltexEvpnIpVrfRowStatus           RowStatus,
      eltexEvpnIpVrfVni                 VniOrNone,
      eltexEvpnIpVrfOperStatus          EltexEvpnOperStatus,
      eltexEvpnIpVrfOperReason          EltexEvpnOperReason,
      eltexEvpnIpVrfOperRd              EltexEvpnRouteDistinguisher,
      eltexEvpnIpVrfName                OCTET STRING
    }

  eltexEvpnIpVrfMrid OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The MRID for which this entry contains information."
    ::= { eltexEvpnIpVrfEntry 1 }

  eltexEvpnIpVrfRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION "Row status. Used to create and delete the configuration
                 for this IP-VRF."
    ::= { eltexEvpnIpVrfEntry 2 }

  eltexEvpnIpVrfVni OBJECT-TYPE
    SYNTAX      VniOrNone
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION "The VXLAN Network Identifier for the IP-VRF.  This is
                 required if the encapsulation is 'vxlan' and ignored
                 otherwise."
    DEFVAL      { 0 }
    ::= { eltexEvpnIpVrfEntry 3 }

  eltexEvpnIpVrfOperStatus OBJECT-TYPE
    SYNTAX      EltexEvpnOperStatus
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Operational status of the IP-VRF configuration."
    ::= { eltexEvpnIpVrfEntry 4 }

  eltexEvpnIpVrfOperReason OBJECT-TYPE
    SYNTAX      EltexEvpnOperReason
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The reason for the IP-VRF configuration to be in its
                 current oper status."
    ::= { eltexEvpnIpVrfEntry 5 }

  eltexEvpnIpVrfOperRd OBJECT-TYPE
    SYNTAX      EltexEvpnRouteDistinguisher
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The route distinguisher in use for this IP-VRF.

                 This is set as follows.

                 -  The value of evpnIpVrfRouteDistinguisher if configured
                    and not set to all zeros.

                 -  Automatically derived if evpnIpVrfRouteDistinguisher is
                    set to all zeros and the IP-VRF has the information
                    required to derive the value.

                    The auto-derived RD is {0x0001, <local IP addr>, VNI}.

                    The <local IP addr> is the configured local router IPv4
                    address, if any, otherwise it is the BGP router ID.

                 -  Set to all zeros otherwise."
    ::= { eltexEvpnIpVrfEntry 6 }

  eltexEvpnIpVrfName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (1..32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The text name of the IP-VRF."
    ::= { eltexEvpnIpVrfEntry 7 }

