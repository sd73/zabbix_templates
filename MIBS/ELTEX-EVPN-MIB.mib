ELTEX-EVPN-MIB DEFINITIONS ::= BEGIN

  IMPORTS
    MODULE-IDENTITY,
    OBJECT-TYPE,
    Unsigned32
      FROM SNMPv2-SMI

    VlanIdOrNone
      FROM RLLAN1-MIB

    TEXTUAL-CONVENTION,
    DisplayString,
    RowStatus,
    MacAddress,
    TruthValue
      FROM SNMPv2-TC

    InterfaceIndexOrZero,
    InterfaceIndex
      FROM IF-MIB

    InetAddressType,
    InetAddress
      FROM INET-ADDRESS-MIB

    NumericIndex
      FROM RADLAN-PIM-MIB

    eltexLtd
      FROM ELTEX-SMI-ACTUAL;

  eltexEvpnMIB MODULE-IDENTITY
    LAST-UPDATED "202108120000Z" -- Th Aug 12 00:00:00 GMT 2021
    ORGANIZATION "Eltex Enterprise Co, Ltd."
    CONTACT-INFO 
        "www.eltex.nsk.ru"
    DESCRIPTION  
        "The MIB module for management of the VxLAN/EVPN protocols."

  ::= { eltexLtd 56 }

-------------------------------------------------------------------------------
-- Textual Conventions.

  VniOrNone ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Virtual Network Identifier."
    SYNTAX  INTEGER (0..16777214)

  EltexEvpnAdminStatus ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "The desired administrative state of an entry."
    SYNTAX      INTEGER {
                  adminStatusUp(1),
                  adminStatusDown(2)
                }

  EsNumberOrNone ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Ethernet Segment number."
    SYNTAX  INTEGER (0..16777214)
    
  EvpnEsType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Ethernet Segment type from RFC7432."
    SYNTAX      INTEGER {
                  esTypeArbitrary(0),
                  esTypeLacp(1),
                  esTypeStp(2),
                  esTypeMac(3),
                  esTypeOperator(4),
                  esTypeAs(5)
                }

  EltexEvpnRouteTarget ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "The representation of route target value."
    SYNTAX       OCTET STRING (SIZE (8))

  EltexEvpnRouteTargetType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "The representation of route target type."
    SYNTAX      INTEGER {
                  routeTargetImport(1),
                  routeTargetExport(2),
                  routeTargetBoth(3)
                }

  EltexEvpnMcastModeType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Locally configured multicast mode."
    SYNTAX      INTEGER {
                  ingressReplication(1), -- Ingress replication.
                  pimAsm(2)              -- PIM-ASM multicast group.
                }

  EltexEvpnEsStatusType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Status values for an Ethernet Segment or interface in an
                 Ethernet Segment."
    SYNTAX      INTEGER {
                  up(1),
                  down(2)
                }

  EltexEvpnEsModeTc ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Operational mode for an Ethernet Segment.
                 Only single-homing and all-active multi-homing modes
                 are supported."
    SYNTAX      INTEGER {
                  singleHomed(1),
                  allActive(2)
                }

  EltexEvpnRouteDistinguisher ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Syntax for a route distinguisher."
    SYNTAX      OCTET STRING (SIZE (8))

  EltexEvpnOperStatus ::= TEXTUAL-CONVENTION
    STATUS current
    DESCRIPTION "Operational status of an EVPN object."
    SYNTAX      INTEGER {
                  operStatusUp(1),       -- active
                  operStatusDown(2),     -- inactive
                  operStatusGoingUp(3),  -- activating
                  operStatusGoingDown(4),-- deactivating
                  operStatusActFailed(5) -- activation failed
                }

  EltexEvpnVxlanOperReason ::= TEXTUAL-CONVENTION
    STATUS current
    DESCRIPTION "A reason for the current oper status of an VxLAN instance."
    SYNTAX      INTEGER {
                  none(1),                 -- Oper status is up or going up.
                                           -- No reason is required.
                  bgpNotUp(2),             -- Oper status is down or going
                                           -- down because BGP protocol is down.
                  adminConfig(3),          -- Oper status is down or going down
                                           -- because of the VxLAN instance row
                                           -- status or admin status
                                           -- configuration.
                  resourceFailure(4),      -- Oper status is failed or failed
                                           -- perm due to insufficient
                                           -- resources.
                  noIfInfo(5),             -- Oper status is down or going down
                                           -- because the VxLAN instance does not have
                                           -- all of the interface information
                                           -- required to activate.
                  vxlanInstanceNotUp(6),   -- Oper status is down or going down
                                           -- because the VxLAN instance is
                                           -- not oper up.
                  noRouteDistinguisher(7), -- Oper status is not up because no
                                           -- route distinguisher is available.
                  routeDistinguisherClash(8),
                                           -- Oper status is not up because
                                           -- there is an active VxLAN instance
                                           -- with the same route
                                           -- distinguisher.
                  noEsi(9),                -- Oper status is not up because no
                                           -- Ethernet Segment Identifier
                                           -- is available.
                  badVni(10),              -- Oper status is not up because a
                                           -- valid VNI is not available.
                  vniClash(11),            -- Oper status is not up because
                                           -- there is another VxLAN instance
                                           -- with the same VNI.
                  vlanSubIfVxlanClash(12), -- Oper status is not up because
                                           -- multiple VLAN sub-interfaces have
                                           -- been configured for the same VxLAN
                                           -- instance on the same Ethernet Segment.
                  noBgpId(13),             -- Oper status is not up because
                                           -- because the BGP router identifier
                                           -- is not yet available to EVPN.
                  rtTypeClash(14),         -- Oper status is not up because a
                                           -- configured RT and the auto-
                                           -- derived RT have the same RT value
                                           -- but different RT types.
                  noRouteTarget(15),       -- Oper status is not up because
                                           -- there are no configured RTs.
                  evpnIpVrfNotUp(16),      -- Oper status is down or going down
                                           -- because the IP VRF instance is
                                           -- not oper up.
                  evpnNoSystemMac(17),     -- Oper status is down or going down
                                           -- because the system mac address is
                                           -- unknown.
                  rtClash(18),             -- Oper status is not up because there
                                           -- is an another active RT configured
                                           -- with the same route target value.
                  evpnNoMcastGroup(19),    -- Oper status is down or going down
                                           -- because the multicast mode
                                           -- requires a group address, but one
                                           -- has not been supplied.
                  evpnNoSourceIp(20),      -- Oper status is down or going down
                                           -- because there is no available
                                           -- source ip.
                  evpnBadTnnlDestType(21), -- Oper status is down or going down
                                           -- because the VxLAN tunnel
                                           -- destination type is invalid.
                  evpnNoRoutersMac(22),    -- Oper status is down or going down
                                           -- because a routers mac is not
                                           -- available.
                  evpnRestartInProgress(23),
                                           -- Oper status is down because there
                                           -- is a restart in progress.
                  evpnNoMcastLabel(24),    -- Oper status is down or going down
                                           -- because MPLS multicast is
                                           -- enabled, but no multicast label
                                           -- exists.
                  evpnNoPerFdbLabel(25),   -- Oper status is down or going down
                                           -- because no per-FDB label exists.
                  evpnNoMplsLabel(26),
                                           -- Oper status is down or going down
                                           -- because an MPLS label is not
                                           -- available.
                  evpnEsiClash(27)
                                           -- Oper status is not up because the
                                           -- ESI for this Ethernet Segment clashes
                                           -- with that of another local Ethernet
                                           -- Segment.
                }

  EltexEvpnOperReason ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "A reason for the current oper status of an EVPN object."
    SYNTAX      INTEGER {
                  none(1),                 -- Oper status is up or going up.
                                           -- No reason is required.
                  entityNotUp(2),          -- Oper status is down or going
                                           -- down because the entity is not
                                           -- oper up.
                  adminConfig(3),          -- Oper status is down or going down
                                           -- because of the object's row
                                           -- status or admin status
                                           -- configuration.
                  resourceFailure(4),      -- Oper status is failed or failed
                                           -- perm due to insufficient
                                           -- resources.
                  noIfInfo(5),             -- Oper status is down or going down
                                           -- because the object does not have
                                           -- all of the interface information
                                           -- required to activate.
                  eviNotUp(6),             -- Oper status is down or going down
                                           -- because the EVPN instance is not
                                           -- oper up.
                  evpnBdNotUp(7),          -- Oper status is down or going down
                                           -- because the EVPN bridge domain is
                                           -- not oper up.
                  noRouteDistinguisher(8), -- Oper status is not up because no
                                           -- route distinguisher is available.
                  routeDistinguisherClash(9),
                                           -- Oper status is not up because
                                           -- there is an active EVPN instance
                                           -- with the same route
                                           -- distinguisher.
                  noEsi(10),               -- Oper status is not up because no
                                           -- Ethernet Segment Identifier
                                           -- is available.
                  badVni(11),              -- Oper status is not up because a
                                           -- valid VNI is not available.
                  vniClash(12),            -- Oper status is not up because
                                           -- there is another BD with the same
                                           -- VNI.
                  vlanSubIfEviClash(13),   -- Oper status is not up because
                                           -- multiple VLAN sub-interfaces have
                                           -- been configured for the same EVI
                                           -- on the same Ethernet Segment.
                  noBgpId(14),             -- Oper status is not up because
                                           -- because the BGP router identifier
                                           -- is not yet available to EVPN.
                  rtTypeClash(15),         -- Oper status is not up because a
                                           -- configured RT and the auto-
                                           -- derived RT have the same RT value
                                           -- but different RT types.
                  noRouteTarget(16),       -- Oper status is not up because
                                           -- there are no configured RTs.
                  evpnIpVrfNotUp(17),      -- Oper status is down or going down
                                           -- because the IP VRF instance is
                                           -- not oper up.
                  evpnNoSystemMac(18),     -- Oper status is down or going down
                                           -- because the system mac address is
                                           -- unknown.
                  rtClash(19),             -- Oper status is not up because there
                                           -- is an another active RT configured
                                           -- with the same route target value.
                  evpnNoMcastGroup(20),    -- Oper status is down or going down
                                           -- because the multicast mode
                                           -- requires a group address, but one
                                           -- has not been supplied.
                  evpnNoSourceIp(21),      -- Oper status is down or going down
                                           -- because there is no available
                                           -- source ip.
                  evpnBadTnnlDestType(22), -- Oper status is down or going down
                                           -- because the VXLAN tunnel
                                           -- destination type is invalid.
                  evpnNoRoutersMac(23),    -- Oper status is down or going down
                                           -- because a routers mac is not
                                           -- available.
                  evpnRestartInProgress(24),
                                           -- Oper status is down because there
                                           -- is arestart in progress.
                  evpnNoMcastLabel(25),    -- Oper status is down or going down
                                           -- because MPLS multicast is
                                           -- enabled, but no multicast label
                                           -- exists.
                  evpnNoPerFdbLabel(26),   -- Oper status is down or going down
                                           -- because no per-FDB label exists.
                  evpnNoMplsLabel(27),
                                           -- Oper status is down or going down
                                           -- because an MPLS label is not
                                           -- available.
                  evpnEsiClash(28)
                                           -- Oper status is not up because the
                                           -- ESI for this Ethernet Segment clashes
                                           -- with that of another local Ethernet
                                           -- Segment.
                }


  EltexEvpnSource ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "The source of an EVPN route.

                 -  'local' indicates that the EVPN route was created based
                    on information from locally attached sites.

                 -  'remote' indicates that the EVPN route was received from
                    a remote EVPN instance."
    SYNTAX      INTEGER {
                  local(1),
                  remote(2)
                }

  EltexEvpnESI ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Syntax for an Ethernet Segment Identifier."
    SYNTAX      OCTET STRING (SIZE (10))

  EltexEvpnPmsiTnnlType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "The tunnel type for forwarding BUM traffic for a
                 bridge domain."
    SYNTAX      INTEGER {
                  ingressReplication(1), -- Ingress replication.
                  pimAsm(2),             -- PIM-ASM multicast group.
                  pimBidir(3),           -- PIM-BIDIR multicast group.
                  pimSsm(4),             -- PIM-SSM multicast group.
                  other(5)               -- Any other type.
               }
-------------------------------------------------------------------------------

  eltexEvpnObjects    OBJECT IDENTIFIER ::= { eltexEvpnMIB 1 }

  eltexEvpnVxlan      OBJECT IDENTIFIER ::= { eltexEvpnObjects 1 }

  eltexEvpnEs         OBJECT IDENTIFIER ::= { eltexEvpnObjects 2 }

  eltexEvpnIpVrf      OBJECT IDENTIFIER ::= { eltexEvpnObjects 3 }

  eltexEvpnRoute      OBJECT IDENTIFIER ::= { eltexEvpnObjects 4 }

--
-- VxLAN instance table.
--
-- This table is responsible for storing configuration of VxLAN instance.
-- All primary configuration of VxLAN instance, including VxLAN and VLAN binding
-- should be made through this table.

  eltexEvpnVxlanTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF EltexEvpnVxlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Each entry of this table represents primary configuration
                 of certain VxLAN instance."
    ::= { eltexEvpnVxlan 1 }

  eltexEvpnVxlanEntry OBJECT-TYPE
    SYNTAX      EltexEvpnVxlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Represents a primary configuration of VxLAN instance."
    INDEX       { eltexEvpnVxlanIndex }
    ::= { eltexEvpnVxlanTable 1 }

  EltexEvpnVxlanEntry ::= SEQUENCE {
      eltexEvpnVxlanIndex                    Unsigned32,
      eltexEvpnVxlanRowStatus                RowStatus,
      eltexEvpnVxlanAdminStatus              EltexEvpnAdminStatus,
      eltexEvpnVxlanVni                      VniOrNone,
      eltexEvpnVxlanVlanId                   VlanIdOrNone,
      eltexEvpnVxlanName                     DisplayString,
      eltexEvpnVxlanMcastMode                EltexEvpnMcastModeType,
      eltexEvpnVxlanMcastGpAddressType       InetAddressType,
      eltexEvpnVxlanMcastGpAddress           InetAddress,
      eltexEvpnVxlanRd                       EltexEvpnRouteDistinguisher,
      eltexEvpnVxlanAutoRt                   EltexEvpnRouteTarget,
      eltexEvpnVxlanOperStatus               EltexEvpnOperStatus,
      eltexEvpnVxlanOperReason               EltexEvpnVxlanOperReason,
      eltexEvpnVxlanVniIpRouting             TruthValue
    }

  eltexEvpnVxlanIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "An index that identifies a VxLAN instance entry."
    ::= { eltexEvpnVxlanEntry 1 }

  eltexEvpnVxlanRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION "The row status of VxLAN Instance table entry. Should be
                 used to create and destroy table entries."
    ::= { eltexEvpnVxlanEntry 2 }

  eltexEvpnVxlanAdminStatus OBJECT-TYPE
    SYNTAX      EltexEvpnAdminStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "The administrative status of VxLAN instance. Should be 
                 used to activate and deactivate instance."
    DEFVAL      { adminStatusUp }
    ::= { eltexEvpnVxlanEntry 3 }

  eltexEvpnVxlanVni OBJECT-TYPE
    SYNTAX      VniOrNone
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "Virtual Network Identifier.
                 Should be used to determine VxLAN segment and encapsulated
                 packet's VNI number."
    ::= { eltexEvpnVxlanEntry 4 }

  eltexEvpnVxlanVlanId OBJECT-TYPE
    SYNTAX      VlanIdOrNone
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "ID of a VLAN interface from which incoming packets
                 should be encapsulated."
    ::= { eltexEvpnVxlanEntry 5 }

  eltexEvpnVxlanName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(1..64))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "A string that identifies VxLAN instance."
    ::= { eltexEvpnVxlanEntry 6 }

  eltexEvpnVxlanMcastMode OBJECT-TYPE
    SYNTAX      EltexEvpnMcastModeType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION "Multicast mode used by this bridge-domain."
    DEFVAL { ingressReplication }
    ::= { eltexEvpnVxlanEntry 7 }

  eltexEvpnVxlanMcastGpAddressType OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION "The type of IP multicast group address to be used for
                 BUM traffic for a VXLAN Bridge Domain. The address is
                 required if eltexEvpnVxlanMcastMode is 'pimAsm'.

                 Note that the multicast configuration must be consistent
                 across all routers in the VNI."
    DEFVAL { unknown }
    ::= { eltexEvpnVxlanEntry 8 }

  eltexEvpnVxlanMcastGpAddress OBJECT-TYPE
    SYNTAX      InetAddress (SIZE(0|4|16))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION "The IP multicast group address to be used for BUM traffic
                 for a VXLAN Bridge Domain.  The address is required if
                 eltexEvpnVxlanMcastMode is 'pimAsm'."
    DEFVAL { ''h }
    ::= { eltexEvpnVxlanEntry 9 }

  eltexEvpnVxlanRd OBJECT-TYPE
    SYNTAX      EltexEvpnRouteDistinguisher
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The route distinguisher in use for this VxLAN instance."
    ::= { eltexEvpnVxlanEntry 10 }

  eltexEvpnVxlanAutoRt OBJECT-TYPE
    SYNTAX      EltexEvpnRouteTarget
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The auto-derived route target for this VxLAN instance.

                 Set to all zeros if EVPN does not have the information
                 required to derive the value."
    ::= { eltexEvpnVxlanEntry 11 }

  eltexEvpnVxlanOperStatus OBJECT-TYPE
    SYNTAX      EltexEvpnOperStatus
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Operational status of the VxLAN instance."
    ::= { eltexEvpnVxlanEntry 12 }

  eltexEvpnVxlanOperReason OBJECT-TYPE
    SYNTAX      EltexEvpnVxlanOperReason
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The reason for the VxLAN instance to be in its current oper
                 status, if the instance is not active or activating."
    ::= { eltexEvpnVxlanEntry 13 }

  eltexEvpnVxlanVniIpRouting OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION "Indicates that vni used for VXLAN encapsulation
                 of IP packets routed in VRF IP-VRF."
    DEFVAL      { false }
    ::= { eltexEvpnVxlanEntry 14 }

-------------------------------------------------------------------------------

--
-- VxLAN name to instance table.
--
-- The purpose of this table is to search VxLAN instance entry index by name.
-- Because VxLAN instances are identified by name in CLI, the search of VxLAN
-- instance entry through SNMP protocol/engine can be inhibited. This table
-- should simplify the search of an entry by allowing user to find VxLAN instance
-- index by name.

  eltexEvpnVxlanNameToIndexTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF EltexEvpnVxlanNameToIndexEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Each entry of this table contains VxLAN instance name and index
                 pairs for auxiliary purposes."
    ::= { eltexEvpnVxlan 2 }

  eltexEvpnVxlanNameToIndexEntry OBJECT-TYPE
    SYNTAX      EltexEvpnVxlanNameToIndexEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Entry that contains VxLAN instance name and index pairs for
                 auxiliary purposes."
    INDEX       { eltexEvpnVxlanNameToIndexName }
    ::= { eltexEvpnVxlanNameToIndexTable 1 }
 
  EltexEvpnVxlanNameToIndexEntry ::= SEQUENCE {
      eltexEvpnVxlanNameToIndexName   DisplayString,
      eltexEvpnVxlanNameToIndexIndex  Unsigned32
    }

  eltexEvpnVxlanNameToIndexName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(1..64))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "A string that identifies VxLAN instance."
    ::= { eltexEvpnVxlanNameToIndexEntry 1 }

  eltexEvpnVxlanNameToIndexIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "An index that identifies a VxLAN instance entry."
    ::= { eltexEvpnVxlanNameToIndexEntry 2 }

-------------------------------------------------------------------------------

--
-- First free index scalar value.
--
-- This scalar contains the next precalculated VxLAN instance index that will
-- be used in case of configuration of VxLAN instance through CLI.

  eltexEvpnVxlanFirstFreeIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The next precalculated VxLAN instance index that will
                 be used in case of configuration of VxLAN instance through CLI."
    ::= { eltexEvpnVxlan 3 }

-------------------------------------------------------------------------------

--
-- VxLAN route target table.
--
-- This table is responsible for storing configuration of route target for certain
-- VxLAN instance. Each VxLAN instance can have multiple route targets configured.

  eltexEvpnVxlanRouteTargetTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF EltexEvpnVxlanRouteTargetEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Each entry of this table represents route target configuration
                 for certain VxLAN instance."
    ::= { eltexEvpnVxlan 4 }

  eltexEvpnVxlanRouteTargetEntry OBJECT-TYPE
    SYNTAX      EltexEvpnVxlanRouteTargetEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Represents route target configuration for certain VxLAN instance."
    INDEX       { eltexEvpnVxlanIndex,
                  eltexEvpnVxlanRouteTarget }
    ::= { eltexEvpnVxlanRouteTargetTable 1 }

  EltexEvpnVxlanRouteTargetEntry ::= SEQUENCE {
      eltexEvpnVxlanRouteTarget           EltexEvpnRouteTarget,
      eltexEvpnVxlanRouteTargetRowStatus  RowStatus,
      eltexEvpnVxlanRouteTargetType       EltexEvpnRouteTargetType
    }

  eltexEvpnVxlanRouteTarget OBJECT-TYPE
    SYNTAX      EltexEvpnRouteTarget
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "8-byte route target value."
    ::= { eltexEvpnVxlanRouteTargetEntry 1 }

  eltexEvpnVxlanRouteTargetRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION "The row status of VxLAN Instance table entry. Should be
                 used to create and destroy table entries."
    ::= { eltexEvpnVxlanRouteTargetEntry 2 }

  eltexEvpnVxlanRouteTargetType OBJECT-TYPE
    SYNTAX      EltexEvpnRouteTargetType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "The type of route target which can be either import, export or both."
    ::= { eltexEvpnVxlanRouteTargetEntry 3 }

-------------------------------------------------------------------------------

--
-- VxLAN tunnel table.
--
-- This table is responsible for information about VxLAN tunnels.

  eltexEvpnVxlanTunnelTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF EltexEvpnVxlanTunnelEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Read-only table for displaying VxLAN tunnels in use by
                 VxLAN instances."
    ::= { eltexEvpnVxlan 5 }

  eltexEvpnVxlanTunnelEntry OBJECT-TYPE
    SYNTAX      EltexEvpnVxlanTunnelEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "A VXLAN tunnel used by EVPN instance."
    INDEX       { eltexEvpnVxlanTunnelLocalVni,
                  eltexEvpnVxlanTunnelRemoteVni,
                  eltexEvpnVxlanTunnelDestinationAddressType,
                  eltexEvpnVxlanTunnelDestinationAddress,
                  eltexEvpnVxlanTunnelSourceAddressType,
                  eltexEvpnVxlanTunnelSourceAddress }
    ::= { eltexEvpnVxlanTunnelTable 1 }

  EltexEvpnVxlanTunnelEntry ::= SEQUENCE {
      eltexEvpnVxlanTunnelLocalVni                VniOrNone,
      eltexEvpnVxlanTunnelRemoteVni               VniOrNone,
      eltexEvpnVxlanTunnelDestinationAddressType  InetAddressType,
      eltexEvpnVxlanTunnelDestinationAddress      InetAddress,
      eltexEvpnVxlanTunnelSourceAddressType       InetAddressType,
      eltexEvpnVxlanTunnelSourceAddress           InetAddress,
      eltexEvpnVxlanTunnelOperStatus              EltexEvpnOperStatus,
      eltexEvpnVxlanTunnelVxlanIndex              Unsigned32,
      eltexEvpnVxlanTunnelLocalVlanId             VlanIdOrNone
    }

  eltexEvpnVxlanTunnelLocalVni OBJECT-TYPE
    SYNTAX      VniOrNone
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The local VNI associated with the VxLAN tunnel."
    ::= { eltexEvpnVxlanTunnelEntry 1 }

  eltexEvpnVxlanTunnelRemoteVni OBJECT-TYPE
    SYNTAX      VniOrNone
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The remote VNI associated with the VxLAN tunnel."
    ::= { eltexEvpnVxlanTunnelEntry 2 }

  eltexEvpnVxlanTunnelDestinationAddressType OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The destination IP address type of the VxLAN tunnel."
    ::= { eltexEvpnVxlanTunnelEntry 3 }

  eltexEvpnVxlanTunnelDestinationAddress OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The destination IP address of the VxLAN tunnel."
    ::= { eltexEvpnVxlanTunnelEntry 4 }

  eltexEvpnVxlanTunnelSourceAddressType OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The source IP address type of the VxLAN tunnel."
