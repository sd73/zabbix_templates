--
-- eltexEvpnRouteMacIpTable
--
-- Read-only table for displaying MAC addresses belonging to EVPN instances.
-- This includes MAC addresses learned from locally attached customer
-- sites and MAC addresses received from remote EVPN instances.
--

  eltexEvpnRouteMacIpTable OBJECT-TYPE
    SYNTAX        SEQUENCE OF EltexEvpnRouteMacIpEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION "Read-only table for displaying MAC addresses belonging to
                 EVPN instances. This includes MAC addresses learned from
                 locally attached customer sites and MAC addresses received
                 from remote EVPN instances."
    ::= { eltexEvpnRoute 1 }

  eltexEvpnRouteMacIpEntry OBJECT-TYPE
    SYNTAX        EltexEvpnRouteMacIpEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION "A MAC address associated with an EVPN instance."
    INDEX { eltexEvpnRouteVpnId,
            eltexEvpnRouteMacIpEthernetTagId,
            eltexEvpnRouteMacIpMacAddress,
            eltexEvpnRouteMacIpInetAddressType,
            eltexEvpnRouteMacIpInetAddress,
            eltexEvpnRouteMacIpPathIndex
            }
      ::= { eltexEvpnRouteMacIpTable 1 }

  EltexEvpnRouteMacIpEntry ::= SEQUENCE {
      eltexEvpnRouteVpnId                      Unsigned32,
      eltexEvpnRouteMacIpEthernetTagId         Unsigned32,
      eltexEvpnRouteMacIpMacAddress            MacAddress,
      eltexEvpnRouteMacIpInetAddressType       InetAddressType,
      eltexEvpnRouteMacIpInetAddress           InetAddress,
      eltexEvpnRouteMacIpPathIndex             NumericIndex,
      eltexEvpnRouteMacIpSource                EltexEvpnSource,
      eltexEvpnRouteMacIpBgpNhAddrType         InetAddressType,
      eltexEvpnRouteMacIpBgpNhAddr             InetAddress,
      eltexEvpnRouteMacIpLocalInterface        InterfaceIndexOrZero,
      eltexEvpnRouteMacIpLabel                 Unsigned32,
      eltexEvpnRouteMacIpInUse                 TruthValue,
      eltexEvpnRouteMacIpEsi                   EltexEvpnESI,
      eltexEvpnRouteMacIpSequenceNum           Unsigned32,
      eltexEvpnRouteMacIpSticky                TruthValue,
      eltexEvpnRouteMacIpDampened              TruthValue
    }

  eltexEvpnRouteVpnId OBJECT-TYPE
    SYNTAX        Unsigned32
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION "VPN ID which is equal to VxLAN index value."
   ::= { eltexEvpnRouteMacIpEntry 1 }

  eltexEvpnRouteMacIpEthernetTagId OBJECT-TYPE
    SYNTAX        Unsigned32
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION "The Ethernet tag ID associated with the MAC address."
   ::= { eltexEvpnRouteMacIpEntry 2 }

  eltexEvpnRouteMacIpMacAddress OBJECT-TYPE
    SYNTAX        MacAddress
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION "The MAC address in the EVPN FDB."
   ::= { eltexEvpnRouteMacIpEntry 3 }

  eltexEvpnRouteMacIpInetAddressType OBJECT-TYPE
    SYNTAX        InetAddressType
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION "Type of IP address associated with the MAC address, or
                 'unknown' if there is no associated IP address."
   ::= { eltexEvpnRouteMacIpEntry 4 }

  eltexEvpnRouteMacIpInetAddress OBJECT-TYPE
    SYNTAX        InetAddress (SIZE(0|4|16))
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION "IP address associated with the MAC address, if any."
   ::= { eltexEvpnRouteMacIpEntry 5 }

  eltexEvpnRouteMacIpPathIndex OBJECT-TYPE
    SYNTAX        NumericIndex
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION "Arbitrary index, used to distinguish multiple paths
                 associated with a single MAC/IP entry."
   ::= { eltexEvpnRouteMacIpEntry 6 }

  eltexEvpnRouteMacIpSource OBJECT-TYPE
    SYNTAX        EltexEvpnSource
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION "Source from which the MAC address was learned."
   ::= { eltexEvpnRouteMacIpEntry 7 }

  eltexEvpnRouteMacIpBgpNhAddrType OBJECT-TYPE
    SYNTAX        InetAddressType
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION "Type of IP address associated with the BGP next hop for
                 the MAC address.

                 Set to 'unknown' for MAC addresses learned from local
                 customer sites."
   ::= { eltexEvpnRouteMacIpEntry 8 }

  eltexEvpnRouteMacIpBgpNhAddr OBJECT-TYPE
    SYNTAX        InetAddress (SIZE(0|4|16))
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION "The BGP next hop for the MAC address.

                 This returns a zero length string for MAC addresses
                 learned from local customer sites."
   ::= { eltexEvpnRouteMacIpEntry 9 }

  eltexEvpnRouteMacIpLocalInterface OBJECT-TYPE
    SYNTAX        InterfaceIndexOrZero
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION "The local interface index through which the MAC address
                 can be reached, or zero for a MAC addresses
                 learned through BGP."
   ::= { eltexEvpnRouteMacIpEntry 10 }

  eltexEvpnRouteMacIpLabel OBJECT-TYPE
    SYNTAX        Unsigned32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION "The label or VNI associated with this MAC address."
   ::= { eltexEvpnRouteMacIpEntry 11 }

  eltexEvpnRouteMacIpInUse OBJECT-TYPE
    SYNTAX        TruthValue
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION "Indicates whether the MAC/IP entry is in use for forwarding.

                 For MAC/IP advertisements from remote EVPN instances,
                 this indicates whether the entry has been accepted
                 and programmed.

                 Always set to 'true' for MAC addresses learned from
                 local customer sites."
   ::= { eltexEvpnRouteMacIpEntry 12 }

  eltexEvpnRouteMacIpEsi OBJECT-TYPE
    SYNTAX        EltexEvpnESI
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION "The Ethernet Segment Identifier advertised with the
                 MAC/IP address."
   ::= { eltexEvpnRouteMacIpEntry 13 }

  eltexEvpnRouteMacIpSequenceNum OBJECT-TYPE
    SYNTAX        Unsigned32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION "The MAC Mobility sequence number associated with the
                 MAC/IP address."
   ::= { eltexEvpnRouteMacIpEntry 14 }

  eltexEvpnRouteMacIpSticky OBJECT-TYPE
    SYNTAX        TruthValue
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION "Whether the Sticky flag is set for this MAC/IP address."
   ::= { eltexEvpnRouteMacIpEntry 15 }

  eltexEvpnRouteMacIpDampened OBJECT-TYPE
    SYNTAX        TruthValue
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION "Whether the MAC/IP address is being dampened (routes for this
                 MAC/IP are ignored due to a duplication)."
   ::= { eltexEvpnRouteMacIpEntry 16 }

